node {
  stage('get'){
    git 'https://github.com/ankie3596/jen_docker-compose.git'
   }
  stage('build'){
    sh 'docker-compose build'
   }
  stage('deploy'){
    docker.withRegistry('https://registry.hub.docker.com','DockerId'){
    def customImage = docker.build('ankimittal/hello')
    customImage.push()
   }
 }
}
